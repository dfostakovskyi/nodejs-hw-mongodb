  "paths": {
    "/contacts": {
      "get": {
        "tags": [
          "Contacts"
        ],
        "summary": "Отримання списку контактів",
        "operationId": "getContacts",
        "description": "Ендпоінт дозволяє отримати список контактів із можливістю фільтрації, сортування та пагінації.",
        "security": [
          {
            "bearerAuth": []
          }
        ],
        "parameters": [
          {
            "name": "page",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 1
            },
            "description": "Номер сторінки для пагінації"
          },
          {
            "name": "perPage",
            "in": "query",
            "required": false,
            "schema": {
              "type": "integer",
              "default": 10
            },
            "description": "Кількість елементів на сторінці"
          },
          {
            "name": "type",
            "in": "query",
            "required": false,
            "schema": {
              "type": "string",
              "enum": [
                "work",
                "home",
                "personal"
              ]
            },
            "description": "Тип контакту (робочий, домашній або особистий)"
          },
          {
            "name": "isFavourite",
            "in": "query",
            "required": false,
            "schema": {
              "type": "boolean"
            },
            "description": "Фільтр за улюбленими контактами"
          }
        ],
        "responses": {
          "200": {
            "description": "Успішне отримання списку контактів",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "id": {
                        "type": "string",
                        "description": "Унікальний ідентифікатор контакту",
                        "example": "65e4decdd286b30065d54af9"
                      },
                      "name": {
                        "type": "string",
                        "description": "Ім'я контакту",
                        "example": "John Doe"
                      },
                      "phoneNumber": {
                        "type": "string",
                        "description": "Номер телефону контакту",
                        "example": "+1234567890"
                      },
                      "email": {
                        "type": "string",
                        "description": "Електронна адреса контакту",
                        "example": "johndoe@example.com"
                      },
                      "isFavourite": {
                        "type": "boolean",
                        "description": "Чи є контакт у списку улюблених",
                        "example": true
                      },
                      "contactType": {
                        "type": "string",
                        "description": "Тип контакту",
                        "enum": [
                          "work",
                          "home",
                          "personal"
                        ],
                        "example": "work"
                      },
                      "createdAt": {
                        "type": "string",
                        "format": "date-time",
                        "description": "Дата створення контакту",
                        "example": "2023-05-01T12:00:00Z"
                      },
                      "updatedAt": {
                        "type": "string",
                        "format": "date-time",
                        "description": "Дата останнього оновлення контакту",
                        "example": "2023-05-02T15:30:00Z"
                      }
                    }
                  }
                }
              }
            }
          },
          "404": {
            "description": "Контакти не знайдено"
          }
